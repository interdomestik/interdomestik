{
  "name": "@interdomestik/web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbo --hostname 127.0.0.1 --port 3000",
    "dev:local": "next dev --webpack --hostname 127.0.0.1 --port 3000",
    "build": "next build --webpack && pnpm check:size",
    "check:size": "node scripts/check-size.mjs",
    "start": "next start",
    "lint": "eslint .",
    "typecheck": "pnpm type-check",
    "type-check": "tsc --noEmit",
    "test:unit": "vitest run",
    "test:e2e": "playwright test",
    "test:e2e:fast": "PLAYWRIGHT_SKIP_SEED=1 playwright test",
    "test:e2e:chromium": "playwright test --project=chromium",
    "test:e2e:chromium:list": "playwright test --project=chromium --reporter=list",
    "test:e2e:chromium:fast": "PLAYWRIGHT_SKIP_SEED=1 playwright test --project=chromium --reporter=list",
    "test:e2e:chromium:workers:8": "playwright test --project=chromium --reporter=list --workers=8",
    "test:e2e:chromium:workers:10": "playwright test --project=chromium --reporter=list --workers=10",
    "test:e2e:chromium:workers:12": "playwright test --project=chromium --reporter=list --workers=12",
    "test:e2e:chromium:shard:1of4": "playwright test --project=chromium --reporter=list --shard=1/4",
    "test:e2e:chromium:shard:2of4": "playwright test --project=chromium --reporter=list --shard=2/4",
    "test:e2e:chromium:shard:3of4": "playwright test --project=chromium --reporter=list --shard=3/4",
    "test:e2e:chromium:shard:4of4": "playwright test --project=chromium --reporter=list --shard=4/4",
    "test:e2e:one-by-one": "PLAYWRIGHT_SKIP_SEED=1 PLAYWRIGHT_MANAGE_SERVER=1 node scripts/run-e2e-one-by-one.mjs",
    "e2e:server": "PADDLE_WEBHOOK_BYPASS_SIGNATURE_IN_DEV=1 NEXT_PUBLIC_APP_URL=http://localhost:3000 BETTER_AUTH_URL=http://localhost:3000 INTERDOMESTIK_AUTOMATED=1 next start --hostname localhost --port 3000",
    "e2e:reuse-server": "PLAYWRIGHT_EXTERNAL_SERVER=1 PLAYWRIGHT_FORCE_SEED=1 FORCE_REGEN_STATE=1 playwright test --project=chromium --workers=1 --reporter=line"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.2",
    "@interdomestik/database": "workspace:*",
    "@interdomestik/domain-activities": "workspace:*",
    "@interdomestik/domain-analytics": "workspace:*",
    "@interdomestik/domain-claims": "workspace:*",
    "@interdomestik/domain-communications": "workspace:*",
    "@interdomestik/domain-country-guidance": "workspace:*",
    "@interdomestik/domain-membership-billing": "workspace:*",
    "@interdomestik/domain-referrals": "workspace:*",
    "@interdomestik/domain-users": "workspace:*",
    "@interdomestik/shared-auth": "workspace:*",
    "@interdomestik/shared-utils": "workspace:*",
    "@interdomestik/ui": "workspace:*",
    "@paddle/paddle-js": "^1.6.1",
    "@paddle/paddle-node-sdk": "^3.5.0",
    "@sentry/nextjs": "^10.32.1",
    "@supabase/supabase-js": "^2.87.1",
    "@tanstack/react-query": "^5.87.4",
    "@types/web-push": "^3.6.4",
    "@upstash/ratelimit": "^2.0.7",
    "@upstash/redis": "^1.35.8",
    "better-auth": "^1.4.6",
    "cookies-next": "^6.1.1",
    "date-fns": "^4.1.0",
    "drizzle-orm": "^0.45.1",
    "import-in-the-middle": "^2.0.1",
    "inngest": "^3.48.1",
    "lucide-react": "^0.561.0",
    "nanoid": "^5.1.6",
    "next": "^16.0.10",
    "next-axiom": "^1.10.0",
    "next-intl": "^4.6.0",
    "pdf-lib": "^1.17.1",
    "pdf-parse": "^1.1.1",
    "posthog-js": "^1.315.1",
    "qrcode": "^1.5.4",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-hook-form": "^7.68.0",
    "recharts": "^3.6.0",
    "require-in-the-middle": "^8.0.1",
    "sonner": "^2.0.7",
    "web-push": "^3.6.7",
    "zod": "^4.2.1"
  },
  "devDependencies": {
    "@axe-core/react": "^4.11.0",
    "@next/bundle-analyzer": "^16.0.10",
    "@next/eslint-plugin-next": "^16.1.0",
    "@playwright/test": "^1.57.0",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^22.19.2",
    "@types/pdf-parse": "^1.1.5",
    "@types/qrcode": "^1.5.6",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.2",
    "@vitest/coverage-v8": "^4.0.16",
    "autoprefixer": "^10.4.20",
    "dotenv": "^17.2.3",
    "eslint": "^9.39.2",
    "eslint-config-next": "^16.0.10",
    "eslint-plugin-boundaries": "^5.3.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "jsdom": "^27.3.0",
    "postcss": "^8.5.0",
    "tailwindcss": "^3.4.19",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5.8.0",
    "typescript-eslint": "^8.25.0",
    "vitest": "^4.0.16"
  }
}

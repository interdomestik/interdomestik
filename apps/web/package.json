{
  "name": "@interdomestik/web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "NEXT_PUBLIC_FORCE_NIPIO_REDIRECT=1 DATABASE_URL=${DATABASE_URL:-postgresql://postgres:postgres@127.0.0.1:54322/postgres} BETTER_AUTH_SECRET=${BETTER_AUTH_SECRET:-test-secret-for-local-dev-only-32chars-minimum} next dev --turbo --hostname 127.0.0.1 --port 3000",
    "build": "next build && pnpm check:size",
    "build:ci": "next build",
    "check:size": "node scripts/check-size.mjs",
    "start": "next start",
    "start:ci": "next start",
    "lint": "eslint .",
    "type-check": "tsc --noEmit",
    "test:unit": "vitest run",
    "test:e2e": "playwright test",
    "e2e:gate": "NEXT_PUBLIC_BILLING_TEST_MODE=1 pnpm run build:ci && NEXT_PUBLIC_BILLING_TEST_MODE=1 playwright test --project=gate-ks-sq --project=gate-mk-mk",
    "e2e:guards": "cd ../.. && bash scripts/e2e-strict-guards.sh",
    "e2e:gate:fast": "NEXT_PUBLIC_BILLING_TEST_MODE=1 playwright test --project=gate-ks-sq --project=gate-mk-mk --max-failures=1",
    "e2e:regression": "playwright test --grep-invert \"@quarantine|@legacy|@smoke\" --project=ks-sq --project=mk-mk",
    "e2e:smoke": "playwright test --grep \"@smoke\" --project=ks-sq --project=mk-mk",
    "e2e:quarantine": "playwright test --grep \"@quarantine|@legacy\"",
    "test:e2e:phase5": "playwright test --project=gate-ks-sq --project=gate-mk-mk --project=smoke apps/web/e2e/golden/agent-pro-leads.spec.ts apps/web/e2e/golden/agent-leads-drawer.spec.ts apps/web/e2e/golden/verification-list-ops.spec.ts apps/web/e2e/golden/verification-details-ops.spec.ts apps/web/e2e/golden/agent-pro-claims.spec.ts apps/web/e2e/golden/agent-pro-claims-messaging.spec.ts apps/web/e2e/golden/rbac-scope.spec.ts apps/web/e2e/golden/agent-pro-claims-rbac.spec.ts",
    "test:smoke": "playwright test --project=smoke --grep '@smoke'"
  },

{
  "name": "@interdomestik/database",
  "version": "0.1.0",
  "private": true,
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": "./src/index.ts",
    "./types": "./src/types.ts",
    "./client": "./src/client.ts",
    "./server": "./src/server.ts",
    "./db": "./src/db.ts",
    "./schema": "./src/schema.ts"
  },
  "scripts": {
    "db:generate": "supabase gen types typescript --local > src/types.ts",
    "db:push": "supabase db push",
    "db:studio": "supabase studio",
    "migrate": "tsx src/migrate.ts",
    "generate": "drizzle-kit generate",
    "push": "drizzle-kit push",
    "studio": "drizzle-kit studio",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@supabase/ssr": "^0.7.0",
    "@supabase/supabase-js": "^2.50.0",
    "drizzle-orm": "^0.45.1",
    "postgres": "^3.4.7"
  },
  "peerDependencies": {
    "next": ">=14.0.0"
  },
  "devDependencies": {
    "dotenv": "^17.2.3",
    "drizzle-kit": "^0.31.8",
    "supabase": "^2.67.0",
    "tsx": "^4.21.0",
    "typescript": "^5.8.0"
  }
}

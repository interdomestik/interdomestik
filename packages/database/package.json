{
  "name": "@interdomestik/database",
  "version": "0.1.0",
  "private": true,
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": "./src/index.ts",
    "./constants": "./src/constants.ts",
    "./types": "./src/types.ts",
    "./client": "./src/client.ts",
    "./server": "./src/server.ts",
    "./db": "./src/db.ts",
    "./schema": "./src/schema.ts",
    "./tenant-security": "./src/tenant-security.ts",
    "./member-number": "./src/member-number.ts"
  },
  "scripts": {
    "db:generate": "echo 'Skipping supabase gen types (using Drizzle)' && exit 0",
    "db:push": "supabase db push",
    "db:studio": "supabase studio",
    "migrate": "tsx src/migrate.ts",
    "generate": "drizzle-kit generate",
    "push": "drizzle-kit push",
    "studio": "drizzle-kit studio",
    "type-check": "tsc --noEmit",
    "seed:e2e": "tsx src/seed-e2e.ts",
    "seed:golden": "tsx src/seed-golden.ts",
    "seed:workload": "tsx src/seed-workload/index.ts",
    "seed:full": "tsx src/seed-full/index.ts",
    "seed:pack:ks": "tsx src/seed-packs/ks-workflow-pack.ts"
  },
  "dependencies": {
    "@supabase/ssr": "^0.8.0",
    "@supabase/supabase-js": "^2.87.1",
    "drizzle-orm": "^0.45.1",
    "postgres": "^3.4.7"
  },
  "peerDependencies": {
    "next": ">=14.0.0"
  },
  "devDependencies": {
    "@types/node": "^22.19.2",
    "dotenv": "^17.2.3",
    "drizzle-kit": "^0.31.8",
    "nanoid": "^5.1.6",
    "supabase": "^2.70.5",
    "tsx": "^4.21.0",
    "typescript": "^5.8.0"
  }
